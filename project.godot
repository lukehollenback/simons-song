; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Simon's Song"
run/main_scene="res://scenes/menu_scene.tscn"
boot_splash/image="res://meta/launch_screen.png"
config/icon="res://meta/icon_64x64.png"

[autoload]

BlackBarHandler="*res://scripts/black_bar_handler.gd"
GlobalHandler="*res://scripts/global_handler.gd"
ScoreHandler="*res://scripts/score_handler.gd"

[display]

window/size/width=600
window/size/height=1024
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/handheld/emulate_touchscreen=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_environment="res://default_env.tres"
